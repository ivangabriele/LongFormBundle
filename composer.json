      "email": "contact@inspired-beings.com",
      "homepage": "http://www.inspired-beings.com"
    }
  ],
  "require": {
    "php": ">=5.5",
    "symfony/symfony": ">=2.6",
    "doctrine/doctrine-bundle": ">=1.5",
    "symfony/console": ">=2.6",
    "symfony/yaml": ">=2.6"
